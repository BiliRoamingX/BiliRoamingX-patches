<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.preference.PreferenceCategory>
        <androidx.preference.Preference
            android:fragment="app.revanced.bilibili.settings.fragments.CustomizeBottomFragment"
            android:key="customize_bottom"
            android:title="@string/biliraoming_customize_bottom_title" />
        <androidx.preference.Preference
            android:fragment="app.revanced.bilibili.settings.fragments.CustomizeHomeTabFragment"
            android:key="home_tab"
            android:title="@string/biliraoming_customize_home_tab_title"
            android:summary="@string/biliraoming_customize_home_tab_summary" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="add_bangumi"
            android:title="@string/biliraoming_add_bangumi_title"
            android:summary="@string/biliraoming_add_bangumi_summary" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="add_movie"
            android:title="@string/biliraoming_add_movie_title"
            android:summary="@string/biliraoming_add_movie_summary" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="add_korea"
            android:title="@string/biliraoming_add_korea_title"
            android:summary="@string/biliraoming_add_korea_summary" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="purify_game"
            android:title="@string/biliroaming_purify_game_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="drawer"
            android:title="@string/biliraoming_drawer_title"
            android:summary="@string/biliraoming_drawer_summary" />
    </androidx.preference.PreferenceCategory>
</androidx.preference.PreferenceScreen>
